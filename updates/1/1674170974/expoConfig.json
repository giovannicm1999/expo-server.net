      "codeSigningCertificate": "./certs/certificate.pem",
      "fallbackToCacheTimeout": 30000,
      "codeSigningMetadata": {
        "keyid": "main",
        "alg": "rsa-v1_5-sha256"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
